{
  "name": "ai-interview-assistant",
  "version": "1.0.0",
  "description": "AI-powered interview practice platform",
  "scripts": {
    "install:all": "cd server && npm install && cd ../client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
